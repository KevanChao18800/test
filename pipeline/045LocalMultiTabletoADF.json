										"treatBooleanAsNumber": false
									}
								}
							},
							"inputs": [
								{
									"referenceName": "DS_MigMultipleTables",
									"type": "DatasetReference",
									"parameters": {
										"paramTablename": {
											"value": "@variables('Tablename')",
											"type": "Expression"
										}

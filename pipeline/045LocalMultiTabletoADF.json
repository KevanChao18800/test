								}
							}
						},
						{
							"name": "Copydata",
							"type": "Copy",
							"dependsOn": [
								{
									"activity": "Set Tablename",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
